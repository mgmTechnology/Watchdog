<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Artikel">
    <xs:complexType>
      <xs:sequence>
		<xs:element name="KaDIS-Artikelsteuerung">
			<xs:annotation>
				<xs:documentation xml:lang="de">Artikelsteuerung, Kopf. Bei "aktiv" bekommt der Artikel informationen. Bei entfernen wird der Artikel komplett aus dem Shop "entfernen" (Minimale Informationen in der XML).</xs:documentation>
            </xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					 <xs:pattern value="aktiv|entfernen"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
        <xs:element type="xs:string" name="Verkaufsplattform"><!-- Kann ignoriert werden --> 
			<xs:annotation>
				<xs:documentation xml:lang="de">Gesetzte Verkaufsplattform, für Multi-Shop Steuerung, diese XML Datei wird dann nur von einem Shop benutzt, der dieses Feld abfragt.</xs:documentation>
            </xs:annotation>
		</xs:element>
        <xs:element type="xs:string" name="supplier_name"><!-- Kann ignoriert werden -->
			<xs:annotation>
				<xs:documentation xml:lang="de">Herstellername des Artikels (nicht Lieferant!)</xs:documentation>
            </xs:annotation>
		</xs:element>
        <xs:element name="active"><!-- Entspricht Aktiv-Status im Web-ERP -->
			<xs:annotation>
				<xs:documentation xml:lang="de">Shop interne Artikelsteuerung, Artikel ist im Shop, bekommt weiterhin Updates durch die XML 0 = deaktiviert, 1 = aktiviert.</xs:documentation>
            </xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:int">
					<xs:pattern value="[0-1]"/>
				 </xs:restriction>
			</xs:simpleType>
		</xs:element>
        <xs:element name="tax"><!-- Deutscher Steuersatz des Artikels in Prozent --> 
			<xs:annotation>
				<xs:documentation xml:lang="de">Steuersatz des Artikels z.b. 7% oder 19%, ohne %</xs:documentation>
            </xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:int">
					<xs:pattern value="[0-9]{1,2}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
		<xs:element name="propertyGroup" minOccurs="0"><!-- Kann ignoriert werden -->
			<xs:annotation>
				<xs:documentation xml:lang="de">Erweiterte Eigenschaften Optionen</xs:documentation>
            </xs:annotation>
			<xs:complexType>
				<xs:sequence>
					<xs:element type="xs:string" name="name" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="de">Name des Eigenschaftsets</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element type="xs:int" name="position" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="de">Position des Eigenschaftsets (Sortierung des Eigenschaftsets zu anderen Eigenschaftsets)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="comparable" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="de">Ist das Set Vergleichbar mit anderen Eigenschaftsets (true:false)</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="true|false|True|False|TRUE|FALSE"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element type="xs:int" name="sortMode" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="de">Sortierungsmodus Eigenschaft Sets 0 - 3 (0=Alphabetisch, 1=Numerisch, 2=Postion)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="propertyValues" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation xml:lang="de">Auflistung der Eigenschaften</xs:documentation>
						</xs:annotation>
						<xs:complexType mixed="true">
							<xs:sequence>
								<xs:element name="entry" minOccurs="1" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation xml:lang="de">Eigenschaft</xs:documentation>
									</xs:annotation>
									<xs:complexType mixed="true">
										<xs:sequence>
											<xs:element type="xs:string" name="value" minOccurs="1" maxOccurs="1">
												<xs:annotation>
													<xs:documentation xml:lang="de">Eigenschaftssame</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:int" name="position" minOccurs="1" maxOccurs="1">
												<xs:annotation>
													<xs:documentation xml:lang="de">Eigenschaftsposition</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:string" name="OptionName" minOccurs="1" maxOccurs="1">
												<xs:annotation>
													<xs:documentation xml:lang="de">Optionsname der Eigenschaft</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:int" name="OptionPosition" minOccurs="1" maxOccurs="1">
												<xs:annotation>
													<xs:documentation xml:lang="de">Optionsposition der Eigenschaft</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element type="xs:string" name="valueNumeric" minOccurs="1" maxOccurs="1">
												<xs:annotation>
													<xs:documentation xml:lang="de">Numerischerwert der Eigenschaft</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>	
				</xs:sequence>	
			</xs:complexType>		
		</xs:element>				
		<xs:element name="SpezificationValues_DE" minOccurs="0"><!-- Kann ignoriert werden -->
			<xs:complexType>
				<xs:sequence>
					<xs:element type="xs:string" name="Spezification" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
		<xs:element name="NaehrwerteValues_DE" minOccurs="0"><!-- Kann ignoriert werden -->
          <xs:annotation>
				<xs:documentation xml:lang="de">Nährwerte Multi-Language für Deutsch</xs:documentation>
            </xs:annotation>
		  <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:string" name="Werte" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="propertyValues" minOccurs="0"><!-- Kann ignoriert werden -->
          <xs:annotation>
				<xs:documentation xml:lang="de">Produktfilter Informationen</xs:documentation>
          </xs:annotation>
		  <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:string" name="SEX" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Geschlechtsangabe für Produkt. Mögliche Werte per Semikolon getrennt hinzufügbar (H; D; U; K) entspricht (Herren; Damen; Unisex; Kinder)</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element type="xs:string" name="COLOR" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Vereinheitlichte Produktfarbe z.b. "blau", "rot" oder ...</xs:documentation>
				</xs:annotation>
			  </xs:element>
			  <xs:element type="xs:string" name="SIZE" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Vereinheitlichte Produktgröße z.b. "XXL", "L" oder ...</xs:documentation>
				</xs:annotation>
			  </xs:element>
			  <xs:element type="xs:string" name="MATERIAL" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Vereinheitlichtes Produktmaterial z.b. "Wolle", "Eisen" oder ...</xs:documentation>
				</xs:annotation>
			  </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="mainDetail">
		  <xs:annotation>
			<xs:documentation xml:lang="de">Hauptartikel Informationen</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:string" name="number"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Kadis Artikelnummer</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element name="prices" minOccurs="0" maxOccurs="unbounded"><!-- KaDIS Verkaufspreis ist EK-Preis vom Web-ERP Möglicherweise für die verschiedenen Dependancen unterschiedliche Kundengruppen-->
                <xs:annotation>
					<xs:documentation xml:lang="de">Artikelpreise, mit Staffelpreise und Kundengruppenpreise</xs:documentation>
				</xs:annotation>
				<xs:complexType mixed="true">
                  <xs:sequence>
                    <xs:element name="customerGroupKey" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="de">Kundengruppe, Standard ist Endkunde mit EK oder H für Händler oder H20 für Händler mit 20% oder ....</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							 <xs:restriction base="xs:string">
								 <xs:pattern value="[a-zA-Z0-9_+-]{0,10}"/>
							 </xs:restriction>
						</xs:simpleType>
					</xs:element>
                    <xs:element type="xs:float" name="price" minOccurs="0"><!-- in diesem Fall immer Nettopreise -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Preis des Artikels. Wenn Endkunde dann in Brutto, sonst immer Netto</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element type="xs:float" name="pseudoPrice" minOccurs="0"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Durchgestrichener Preis des Artikels. Wenn Endkunde dann in Brutto, sonst immer Netto</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element type="xs:float" name="basePrice" minOccurs="0"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Grundpreis des Artikels, nicht sichtbar für den Kunden, für Statisik . Immer Netto</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element type="xs:int" name="from" minOccurs="0"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Staffelung, Preis gilt von X (int) (min. 1)</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element type="xs:string" name="to" minOccurs="0"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Staffelung, Preis gilt bis Y (int) (bis unendlich = "beliebig")</xs:documentation>
						</xs:annotation>
					</xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element type="xs:string" name="supplierNumber" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Herstellernummer aus Kadis</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element type="xs:int" name="kind" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Wenn Hauptartikel dann immer 1, wenn nicht 2 für Variante</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element name="active" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Shop interne Hauptartikelsteuerung, 0 = deaktiviert, 1 = aktiviert.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:pattern value="[0-1]"/>
					</xs:restriction>
				</xs:simpleType>
			  </xs:element>
              <xs:element type="xs:int" name="inStock" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Normaler Lagerbestand des Hauptartikels</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element type="xs:int" name="stockMin" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Mindestlagerbestand des Hauptartikels</xs:documentation>
				</xs:annotation>
			  </xs:element>
			  <xs:element name="lastStock" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Abverkaufartikel, wenn vorhanden und Wert "1" dann Abverkauf, gilt auch für alle Varianten</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:pattern value="[0-1]"/>
					</xs:restriction>
				</xs:simpleType>
			  </xs:element>
			  <xs:element name="notification" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">eMail Benachrichtigungsfunktion für Kunden beim Artikel bereitstellen.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:pattern value="[0-1]"/>
					</xs:restriction>
				</xs:simpleType>
			  </xs:element> 
              <xs:element type="xs:float" name="weight" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gewicht des Artikels in kg</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element type="xs:float" name="width" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Breite des Artikels</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element type="xs:float" name="len" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Länge des Artikels</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element type="xs:float" name="height" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Höhe des Artikels</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element name="ean" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">EAN Nummer des Artikels</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:pattern value="[0-9]{8,13}"/>
					</xs:restriction>
				</xs:simpleType>
			  </xs:element> 
              <xs:element type="xs:int" name="position" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Sortierungsnummer, Standard = 0 unsortiert. Shopinterne zb. nach Alphabet</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:int" name="minPurchase" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Mindestbestellmenge</xs:documentation>
				</xs:annotation>
			  </xs:element> 
			  <xs:element type="xs:int" name="purchaseSteps" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Mengenstaffelung / Verkaufsstaffelung (z.b. 2,4,6,8...)</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:int" name="maxPurchase" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Maximale Bestellmenge</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:float" name="purchaseUnit" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Grundpreis: Bestellmenge z.B. 0.75> Stammpreis-Errechnung</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:float" name="referenceUnit" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Grundpreis: Bezugs- / Referenzmenge z.B. 1.00> Stammpreis-Errechnung</xs:documentation>
				</xs:annotation>
			  </xs:element>
              <xs:element name="unit" minOccurs="0"><!-- ist immer St im Web-ERP = Stk -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Verkaufseinheit</xs:documentation>
				</xs:annotation>
				<xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:string" name="unit" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="de">Kürzel z.b. Stück,  l, ml, kg, g, usw.</xs:documentation>
						</xs:annotation>
					</xs:element> 
                    <xs:element type="xs:string" name="name" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="de">Name z.b. Stück, Liter, Mililiter, Kilogramm, Gramm, usw.</xs:documentation>
						</xs:annotation>
					</xs:element> 
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
			  <xs:element type="xs:string" name="unitID" default="Stück" minOccurs="0"> <!-- ist immer St im Web-ERP = Stk -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Fallback unit "Stück"</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:string" name="packUnit" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Ausgabe Bestelleinheit  Stück</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element name="shippingFree" minOccurs="0"> <!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Versandkostenfreier Artikel, true/false</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="true|false"/>
					</xs:restriction>
				</xs:simpleType>
			  </xs:element>
			  <xs:element type="xs:string" name="releaseDate" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Erscheinungsdatum  0000-00-00</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:int" name="shippingTime" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Lieferzeit in Tagen</xs:documentation>
				</xs:annotation>
			  </xs:element> 
				<xs:element name="attribute" minOccurs="0"> <!-- Kann ignoriert werden -->
                <xs:annotation>
					<xs:documentation xml:lang="de">Hauptartikel Attribute</xs:documentation>
				</xs:annotation>
					<xs:complexType>
						<xs:sequence minOccurs="0" maxOccurs="unbounded">
							<xs:any processContents="lax"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ImageAttribute" minOccurs="0"> <!-- Kann ignoriert werden -->
                <xs:annotation>
					<xs:documentation xml:lang="de">Artikel Bilder Attribute</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Image" minOccurs="0" maxOccurs="unbounded"><!-- Kann ignoriert werden -->
							<xs:annotation>
								<xs:documentation xml:lang="de">Bild Informationen zum Artikel</xs:documentation>
							</xs:annotation>
							<xs:complexType mixed="true">
								<xs:sequence>
									<xs:element type="xs:string" name="KadisImageName" minOccurs="0"><!-- Kann ignoriert werden -->
										<xs:annotation>
											<xs:documentation xml:lang="de">Kadis Bildname und Erkennungsname des Bildes</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element type="xs:string" name="ShopImageName" minOccurs="0"><!-- Kann ignoriert werden -->
										<xs:annotation>
											<xs:documentation xml:lang="de">Alternativer Bildname</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element type="xs:string" name="ShopImageTitle" minOccurs="0"><!-- Kann ignoriert werden -->
										<xs:annotation>
											<xs:documentation xml:lang="de">Titel für Bild</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element type="xs:string" name="additionaltext" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Zusatzinformationen, bei Varianten sind es die Ausprägungen z.b. XXL / Grün</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element name="configuratorOptions" minOccurs="0"> <!-- Kann ignoriert werden -->
                <xs:annotation>
					<xs:documentation xml:lang="de">Varianten Informationen, Ausprägungen</xs:documentation>
				</xs:annotation>
				<xs:complexType>
                  <xs:sequence>
                    <xs:element name="Option" maxOccurs="unbounded" minOccurs="1"><!-- Kann ignoriert werden -->
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:string" name="groupIdName">
							<xs:annotation>
								<xs:documentation xml:lang="de">Variantenausprägungsgruppe des Hauptartikels</xs:documentation>
							</xs:annotation>
						  </xs:element> 
                          <xs:element type="xs:string" name="name"><!-- Kann ignoriert werden -->
							<xs:annotation>
								<xs:documentation xml:lang="de">Variantenausprägungsname des Hauptartikels</xs:documentation>
							</xs:annotation>
						  </xs:element>  
                          <xs:element type="xs:int" name="position"><!-- Kann ignoriert werden -->
							<xs:annotation>
								<xs:documentation xml:lang="de">Variantenausprägungssortierung</xs:documentation>
							</xs:annotation>
						  </xs:element> 
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element type="xs:string" name="name" minOccurs="0"><!-- Artikelname (deutsch)-->
			<xs:annotation>
				<xs:documentation xml:lang="de">Artikelname</xs:documentation>
			</xs:annotation>
		</xs:element> 
		<xs:element type="xs:string" name="metaTitle" minOccurs="0"><!-- Kann ignoriert werden -->
			<xs:annotation>
				<xs:documentation xml:lang="de">Meta Titel des Produktes. Wenn nicht gegeben, wird der Artikelnname genommen. 55 Zeichen sind optimal.</xs:documentation>
			</xs:annotation>
		</xs:element>
        <xs:element type="xs:string" name="description" minOccurs="0"><!-- Artikelbeschreibung (deutsch)-->
			<xs:annotation>
				<xs:documentation xml:lang="de">Artikelkurzbeschreibung</xs:documentation>
			</xs:annotation>
		</xs:element> 
        <xs:element type="xs:string" name="descriptionLong" minOccurs="0"><!-- Kann ignoriert werden -->
			<xs:annotation>
				<xs:documentation xml:lang="de">Artikellangbeschreibung</xs:documentation>
			</xs:annotation>
		</xs:element> 
		<xs:element type="xs:string" name="keywords" minOccurs="0"><!-- Kann ignoriert werden -->
			<xs:annotation>
