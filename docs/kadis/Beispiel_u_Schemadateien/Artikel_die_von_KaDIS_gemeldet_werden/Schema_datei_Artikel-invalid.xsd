			  <xs:element name="VarNaehrwerteValues_DE" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Nährwerte Multi-Language für Deutsch</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element type="xs:string" name="Werte" minOccurs="0" maxOccurs="unbounded"/><!-- Kann ignoriert werden -->
					</xs:sequence>
				</xs:complexType>
              </xs:element>
              <xs:element name="VarPropertyValues" minOccurs="0">   <!-- Kann ignoriert werden --> 
                <xs:annotation>
					<xs:documentation xml:lang="de">Produktfilter Informationen der Variante</xs:documentation>
				</xs:annotation>
				<xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:string" name="SEX" minOccurs="0"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Geschlechtsangabe für Produkt. Mögliche Werte per Semikolon getrennt hinzufügbar (H; D; U; K) entspricht (Herren; Damen; Unisex; Kinder)</xs:documentation>
						</xs:annotation>
					</xs:element> 
					<xs:element type="xs:string" name="COLOR" minOccurs="0"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Vereinheitlichte Produktfarbe z.b. "blau", "rot" oder ...</xs:documentation>
						</xs:annotation>
					</xs:element> 
					<xs:element type="xs:string" name="SIZE" minOccurs="0"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Vereinheitlichte Produktgröße z.b. "XXL", "L" oder ...</xs:documentation>
						</xs:annotation>
					</xs:element> 
					<xs:element type="xs:string" name="MATERIAL" minOccurs="0"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Vereinheitlichtes Produktmaterial z.b. "Wolle", "Eisen" oder ...</xs:documentation>
						</xs:annotation>
					</xs:element>  
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="active">  <!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Variante aktiv Status</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">deaktiviert</xs:documentation>
							</xs:annotation>
						</xs:enumeration> 
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="de">aktivert</xs:documentation>
							</xs:annotation>
						</xs:enumeration> 
						<xs:enumeration value="entfernen">
							<xs:annotation>
								<xs:documentation xml:lang="de">Komplett aus dem Shop entfernen</xs:documentation>
							</xs:annotation>
						</xs:enumeration> 
					</xs:restriction>
				</xs:simpleType>
			  </xs:element>
              <xs:element type="xs:int" name="inStock"  minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Lagerbestand der Variante</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:int" name="stockMin"  minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Mindestbestand der Variante</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:float" name="weight"  minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Gewicht der Variante</xs:documentation>
				</xs:annotation>
			  </xs:element>  
              <xs:element type="xs:float" name="width"  minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Breite der Variante</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:float" name="len"  minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Länge der Variante</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:float" name="height"  minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Höhe der Variante</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element name="ean"  minOccurs="0"> <!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">EAN der Variante</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:pattern value="[0-9]{8,13}"/>
					</xs:restriction>
				</xs:simpleType>
			  </xs:element> 
              <xs:element type="xs:int" name="position"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Position der Variante</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:int" name="minPurchase"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">mindest Bestellmenge der Variante</xs:documentation>
				</xs:annotation>
			  </xs:element> 
			  <xs:element type="xs:int" name="purchaseSteps" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Pseudo-Verkäufe / Beeinflussung Topseller-Anzeige</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:int" name="maxPurchase"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">max. Bestellmenge der Variante</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:float" name="purchaseUnit"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Grundpreis: Bestellmenge z.B. 0.75> Stammpreis-Errechnung</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:float" name="referenceUnit"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Grundpreis: Bezugs- / Referenzmenge z.B. 1.00> Stammpreis-Errechnung</xs:documentation>
				</xs:annotation>
			  </xs:element>    
              <xs:element name="unit" minOccurs="0"> <!-- Kann ignoriert werden -->
                <xs:annotation>
					<xs:documentation xml:lang="de">Verkaufseinheit</xs:documentation>
				</xs:annotation>
				<xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:string" name="unit"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Kürzel z.b. Stück,  l, ml, kg, g, usw.</xs:documentation>
						</xs:annotation>
					</xs:element> 
                    <xs:element type="xs:string" name="name"><!-- Kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Name z.b. Stück, Liter, Mililiter, Kilogramm, Gramm, usw.</xs:documentation>
						</xs:annotation>
					</xs:element> 
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
			  <xs:element type="xs:string" name="unitID" default="Stück" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Fallback unit "Stück"</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:string" name="packUnit"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Ausgabe Bestelleinheit  Stück</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element name="shippingFree"> <!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Versandkostenfreier Artikel, true/false</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="true|false"/>
					</xs:restriction>
				</xs:simpleType>
			  </xs:element>
			  <xs:element type="xs:string" name="releaseDate" minOccurs="0"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Erscheinungsdatum  0000-00-00</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element type="xs:int" name="shippingTime" default="5"><!-- Kann ignoriert werden -->
				<xs:annotation>
					<xs:documentation xml:lang="de">Lieferzeit in Tagen</xs:documentation>
				</xs:annotation>
			  </xs:element> 
              <xs:element name="attribute"><!-- Kann ignoriert werden -->
                <xs:complexType>
					<xs:sequence minOccurs="0" maxOccurs="unbounded">
						<xs:any processContents="lax"/>
					</xs:sequence>
                </xs:complexType>
              </xs:element>
			  <xs:element name="ImageAttribute" minOccurs="0"> <!-- Kann ignoriert werden -->
                <xs:annotation>
					<xs:documentation xml:lang="de">Artikel Bilder Attribute</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Image" minOccurs="0" maxOccurs="unbounded"><!-- Kann ignoriert werden -->
							<xs:annotation>
								<xs:documentation xml:lang="de">Bild Informationen zum Artikel</xs:documentation>
							</xs:annotation>
							<xs:complexType mixed="true">
								<xs:sequence>
									<xs:element type="xs:string" name="KadisImageName" minOccurs="0"><!-- Kann ignoriert werden -->
										<xs:annotation>
											<xs:documentation xml:lang="de">Kadis Bildname und Erkennungsname des Bildes</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element type="xs:string" name="ShopImageName" minOccurs="0"><!-- Kann ignoriert werden -->
										<xs:annotation>
											<xs:documentation xml:lang="de">Alternativer Bildname</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element type="xs:string" name="ShopImageTitle" minOccurs="0"><!-- Kann ignoriert werden -->
										<xs:annotation>
											<xs:documentation xml:lang="de">Titel für Bild</xs:documentation>
										</xs:annotation>
									</xs:element> 
									<xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="prices" minOccurs="1" maxOccurs="unbounded"> <!-- unter den Preisen sind fuer die Dependance nur zwei Preise wichtig, das eine ist der Einkaufspreis das andere der empfohlene Verkaufspreis-->
                <xs:annotation>
					<xs:documentation xml:lang="de">Artikelpreise, mit Staffelpreise und Kundengruppenpreise</xs:documentation>
				</xs:annotation>
				<xs:complexType mixed="true">
                  <xs:sequence>
                    <xs:element name="customerGroupKey" minOccurs="0"><!-- hier wird noch definiert wie genau die Preisgruppe fuer eine bestimmte Dependance heißt und wie die Preisgruppe fuer den empfohlenen VK heisst -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Kundengruppe, Standard ist Endkunde mit EK oder H für Händler oder H20 für Händler mit 20% oder</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							 <xs:restriction base="xs:string">
								 <xs:pattern value="[a-zA-Z0-9_+-]{0,10}"/>
							 </xs:restriction>
						</xs:simpleType>
					</xs:element>
                    <xs:element type="xs:float" name="price" minOccurs="0"><!-- Der Dependance Einkaufspreis ist vermutlich in Netto der empfohlene VK-Preis vermutlich in Brutto -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Preis des Artikels. Wenn Endkunde dann in Brutto, sonst immer Netto</xs:documentation>
						</xs:annotation>
					</xs:element> 
                    <xs:element type="xs:float" name="pseudoPrice" minOccurs="0"><!-- kann vermutlich ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Durchgestrichener Preis des Artikels. Wenn Endkunde dann in Brutto, sonst immer Netto</xs:documentation>
						</xs:annotation>
					</xs:element> 
                    <xs:element type="xs:float" name="basePrice" minOccurs="0"><!-- kann ignoriert werden -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Grundpreis des Artikels, nicht sichtbar für den Kunden, für Statisik . Immer Netto</xs:documentation>
						</xs:annotation>
					</xs:element> 
                    <xs:element type="xs:int" name="from" minOccurs="0"><!-- nur falls es in MyCorazon beruecksichtigt werden kann -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Staffelung, Preis gilt von X (int) (min. 1)</xs:documentation>
						</xs:annotation>
					</xs:element> 
                    <xs:element type="xs:string" name="to" minOccurs="0"><!-- nur falls es in MyCorazon beruecksichtigt werden kann -->
						<xs:annotation>
							<xs:documentation xml:lang="de">Staffelung, Preis gilt bis Y (int) (bis unendlich = "beliebig")</xs:documentation>
						</xs:annotation>
					</xs:element> 
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="configuratorOptions"> <!-- kann ignoriert werden -->
                <xs:annotation>
					<xs:documentation xml:lang="de">Varianten Informationen, Ausprägungen</xs:documentation>
				</xs:annotation>
				<xs:complexType>
                  <xs:sequence>
                    <xs:element name="Option" maxOccurs="unbounded" minOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:string" name="groupIdName"><!-- kann ignoriert werden -->
							<xs:annotation>
								<xs:documentation xml:lang="de">Variantenausprägungsgruppe der Variante</xs:documentation>
							</xs:annotation>
						  </xs:element> 
                          <xs:element type="xs:string" name="name"><!-- kann ignoriert werden -->
							<xs:annotation>
								<xs:documentation xml:lang="de">Variantenausprägungsname der Variante</xs:documentation>
							</xs:annotation>
						  </xs:element> 
                          <xs:element type="xs:int" name="position"><!-- kann ignoriert werden -->
							<xs:annotation>
								<xs:documentation xml:lang="de">Variantenausprägungssortierung</xs:documentation>
							</xs:annotation>
						  </xs:element> 
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
