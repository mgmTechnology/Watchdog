							<xs:element name="OrderList">
								<!-- Positionsliste Anfang -->
								<xs:complexType>
									<xs:sequence>
									<xs:element name="Artikel" minOccurs="1" maxOccurs="unbounded">
										<!-- Artikel Position Anfang -->
										<xs:complexType>
											<xs:sequence>
												<xs:element type="xs:string" name="ArtikelNr">
													<xs:annotation>
														<xs:documentation xml:lang="de">Kadis Artikelnummer des Produkts</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element type="xs:integer" name="Menge">
													<xs:annotation>
														<xs:documentation xml:lang="de">Bestellte Menge des Produkts</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element type="xs:string" name="Groesse">
													<xs:annotation>
														<xs:documentation xml:lang="de">Großenangabe des Artikels. STANDARD: St </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element type="xs:float" name="Gewicht" minOccurs="0">
													<xs:annotation>
														<xs:documentation xml:lang="de">Einzelgewicht des Produktes</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element type="xs:string" name="Einheit">
													<xs:annotation>
														<xs:documentation xml:lang="de">Verkaufseinheit des Artikels STANDARD: Stück, Bei Versandkosten : 0</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element type="xs:string" name="Name">
													<xs:annotation>
														<xs:documentation xml:lang="de">Name des Artikels</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element type="xs:string" name="Code" minOccurs="0">
													<xs:annotation>
														<xs:documentation xml:lang="de">Gutschein-Codes für den Druck</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="Preis">
													<xs:annotation>
														<xs:documentation xml:lang="de">Einzelpreis des Artikels mit Komma.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:pattern value="(|-)\d+(,|.\d+)?"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Steuer">
													<xs:annotation>
														<xs:documentation xml:lang="de">Steuersatz des Artikels STANDARD: 19 oder 7 </xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:pattern value="(|-)\d+(,|.\d+)?"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="SteuerEinstellung">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="I">
																<xs:annotation>
																	<xs:documentation xml:lang="de">Preis ist inkl. Steuern</xs:documentation>
																</xs:annotation>
															</xs:enumeration>
															<xs:enumeration value="+">
																<xs:annotation>
																	<xs:documentation xml:lang="de">Preis ist exkl. Steuern</xs:documentation>
																</xs:annotation>
															</xs:enumeration>
															<xs:enumeration value="O">
																<xs:annotation>
																	<xs:documentation xml:lang="de">Preis ist ohne Steuern</xs:documentation>
																</xs:annotation>
															</xs:enumeration>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
										<!-- Artikel Position Ende -->
									</xs:element>
									</xs:sequence>
								</xs:complexType>
								<!-- Positionsliste Ende -->
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>